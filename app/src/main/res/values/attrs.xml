<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AbsSwipeLayout">
        <attr name="restartOnFinish" format="boolean" />
        <attr name="swipeType" format="enum" >
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
            <enum name="both" value="3"/>
            <enum name="none" value="4"/>
        </attr>
        <attr name="swipeLeftMargin" format="dimension"/>
        <attr name="swipeRightMargin" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="SwipeableListView">
        <attr name="restartOnFinish1" format="boolean" />
        <attr name="swipeType1" format="enum" >
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
            <enum name="both" value="3"/>
            <enum name="none" value="4"/>
        </attr>
        <attr name="swipeLeftMargin1" format="dimension"/>
        <attr name="swipeRightMargin1" format="dimension"/>
    </declare-styleable>
</resources>
